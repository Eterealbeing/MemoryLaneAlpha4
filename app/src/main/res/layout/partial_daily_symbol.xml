<?xml version="1.0" encoding="utf-8"?>
<me.blog.korn123.easydiary.views.FixedCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:contentPadding="@dimen/dashboard_symbol_padding"
    app:fixedAppcompatPadding="true"
    app:applyCardBackgroundColor="false"
    app:dashboardInnerCard="true"
    >

    <ImageView
        android:id="@+id/dailySymbol"
        android:layout_width="@dimen/dashboard_symbol_small"
        android:layout_height="@dimen/dashboard_symbol_small"
        app:srcCompat="@drawable/ic_sunny" />
</me.blog.korn123.easydiary.views.FixedCardView>

